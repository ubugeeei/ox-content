{
  "name": "@ox-content/vite-plugin-svelte",
  "version": "0.9.0",
  "description": "Svelte integration for Ox Content - Embed Svelte components in Markdown",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsgo --noEmit"
  },
  "peerDependencies": {
    "svelte": "catalog:",
    "vite": "catalog:"
  },
  "dependencies": {
    "@ox-content/islands": "workspace:*",
    "@ox-content/vite-plugin": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "catalog:",
    "@types/node": "catalog:",
    "svelte": "catalog:",
    "tsdown": "catalog:",
    "@typescript/native-preview": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "svelte",
    "markdown",
    "ox-content"
  ],
  "license": "MIT",
  "author": "ubugeeei",
  "repository": {
    "type": "git",
    "url": "https://github.com/ubugeeei/ox-content.git",
    "directory": "npm/vite-plugin-ox-content-svelte"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  }
}
