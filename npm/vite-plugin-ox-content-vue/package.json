{
  "name": "@ox-content/vite-plugin-vue",
  "version": "0.9.0",
  "description": "Vue integration for Ox Content - Embed Vue components in Markdown",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsgo --noEmit"
  },
  "peerDependencies": {
    "vite": "catalog:",
    "vue": "catalog:"
  },
  "dependencies": {
    "@ox-content/islands": "workspace:*",
    "@ox-content/vite-plugin": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "tsdown": "catalog:",
    "@typescript/native-preview": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vue": "catalog:"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "vue",
    "markdown",
    "ox-content",
    "mdx"
  ],
  "license": "MIT",
  "author": "ubugeeei",
  "repository": {
    "type": "git",
    "url": "https://github.com/ubugeeei/ox-content.git",
    "directory": "npm/vite-plugin-ox-content-vue"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  }
}
