{
  "name": "unplugin-ox-content",
  "version": "0.0.0",
  "description": "Universal plugin for Ox Content - Markdown processing for webpack, rollup, esbuild, vite, and more",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "types": "./dist/vite.d.ts"
    },
    "./webpack": {
      "import": "./dist/webpack.js",
      "types": "./dist/webpack.d.ts"
    },
    "./rollup": {
      "import": "./dist/rollup.js",
      "types": "./dist/rollup.d.ts"
    },
    "./esbuild": {
      "import": "./dist/esbuild.js",
      "types": "./dist/esbuild.d.ts"
    },
    "./rspack": {
      "import": "./dist/rspack.js",
      "types": "./dist/rspack.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && tsc",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ox-content/napi": "workspace:*",
    "@rollup/pluginutils": "^5.1.4",
    "unplugin": "^2.1.2"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "webpack": "^5.97.1",
    "rollup": "^4.29.1",
    "esbuild": "^0.24.2"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "esbuild",
    "markdown",
    "ox-content"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ubugeeei/ox-content.git",
    "directory": "packages/unplugin-ox-content"
  }
}
