{
  "name": "vite-plugin-ox-content-vue",
  "version": "0.0.0",
  "description": "Vue integration for Ox Content - Embed Vue components in Markdown",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./runtime": {
      "import": "./dist/runtime.js",
      "types": "./dist/runtime.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vite": "^6.0.0",
    "vue": "^3.5.0"
  },
  "dependencies": {
    "vite-plugin-ox-content": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vue": "^3.5.0"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "vue",
    "markdown",
    "ox-content",
    "mdx"
  ],
  "license": "MIT"
}
